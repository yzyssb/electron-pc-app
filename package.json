{
  "name": "百度",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "nsis": {
    "oneClick": false,
    "guid": "idea",
    "perMachine": true,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "build/icons/icon.ico",
    "uninstallerIcon": "build/icons/icon.ico",
    "installerHeaderIcon": "build/icons/icon.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "idea"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^19.0.5",
    "electron-builder": "^23.1.0",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^12.2.0"
  },
  "scripts": {
    "start": "electron .",
    "packager": "electron-packager . 百度 --out fukaiitapp --arch=x64 --icon=D:\\新建文件夹\\electron-quick-start\\build\\icons\\icon.ico --overwrite --ignore=node_modules",
    "electron:generate-icons": "electron-icon-builder --input=./girl.png --output=build --flatten",
	"dist": "electron-builder",
	"dist-win": "electron-builder --win --ia32"
  }
}
